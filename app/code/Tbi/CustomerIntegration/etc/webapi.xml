<?xml version="1.0" ?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
	<route method="POST" url="/V1/tbi-customerintegration/addcustomer">
		<service class="Tbi\CustomerIntegration\Api\AddCustomerManagementInterface" method="postAddCustomer"/>
		<resources>
			<resource ref="anonymous"/>
		</resources>
	</route>
	<route method="PUT" url="/V1/tbi-customerintegration/updatecustomer">
		<service class="Tbi\CustomerIntegration\Api\UpdateCustomerManagementInterface" method="putUpdateCustomer"/>
		<resources>
			<resource ref="anonymous"/>
		</resources>
	</route>
	<route method="GET" url="/V1/tbi-customerintegration/getcustomer">
		<service class="Tbi\CustomerIntegration\Api\GetCustomerManagementInterface" method="getGetCustomer"/>
		<resources>
			<resource ref="anonymous"/>
		</resources>
	</route>
	<route method="DELETE" url="/V1/tbi-customerintegration/deletecustomer">
		<service class="Tbi\CustomerIntegration\Api\DeleteCustomerManagementInterface" method="deleteDeleteCustomer"/>
		<resources>
			<resource ref="anonymous"/>
		</resources>
	</route>
</routes>
